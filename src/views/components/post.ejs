<div class="card post my-4">

  <div class="card-header p-4">
  </div>

  <div class="card-body p-4">

    <div class="post-title">
      <h2><%= post.title %></h2>
    </div>

    <div class="post-info">
    </div>

    <div class="post-content">
      <div class="markdown-body">
        <%- post.mdContent %>
      </div>
    </div>

  </div>

  <div class="card-footer p-4">
  </div>

</div>